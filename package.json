{
	"name": "chat",
	"version": "1.0.1",
	"main": "./app/main.js",
	"scripts": {
		"start": "electron .",
		"dev": "webpack -d --watch",
		"build": "webpack",
		"dist": "build",
		"start:server": "http-server dist -p 8080"
	},
	"build": {
		"appId": "com.example.chat",
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true
		},
		"publish": [
			{
				"provider": "generic",
				"url": "http://127.0.0.1:8080/"
			}
		]
	},
	"dependencies": {
		"electron-debug": "^1.1.0",
		"electron-devtools-installer": "^2.2.0",
		"electron-updater": "^2.8.7",
		"http-server": "^0.10.0",
		"nodejs-websocket": "^1.7.1",
		"photonkit": "^0.1.2",
		"prop-types": "^15.5.7",
		"react": "^15.4.2",
		"react-dom": "^15.4.2",
		"react-timeago": "^3.1.3"
	},
	"devDependencies": {
		"babel-core": "^6.22.1",
		"babel-loader": "^6.2.10",
		"babel-plugin-transform-class-properties": "^6.23.0",
		"babel-plugin-transform-object-rest-spread": "^6.23.0",
		"babel-preset-es2017": "^6.22.0",
		"babel-preset-react": "^6.22.0",
		"css-loader": "^0.28.0",
		"devtron": "^1.4.0",
		"electron": "^1.6.2",
		"electron-builder": "^19.24.1",
		"file-loader": "^0.11.1",
		"style-loader": "^0.16.1",
		"webpack": "^2.2.1"
	}
}
